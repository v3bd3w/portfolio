{
	"manifest_version": 2
	,"name": "Extension template"
	,"version": "1.0"

	,"description": "Template of firefox extension with basis functions"

	,"icons": {
		"48": "icons/icon-48.png"
	}
	
	,"background": {
		"scripts": ["background.js"]
	}
	
	,"browser_action": {
		"default_icon": {
			"16": "icons/icon16.png"
			,"32": "icons/icon32.png"
		}
		,"default_title": "Template button"
	}
	
	,"permissions": [
		"*://duckduckgo.com/*"
	]
}

